{
  "from": "academic-credentials-dataset",
  "select": [
    "(count ?learners)",
    "(count ?institutions)",
    "(count ?employers)",
    "(count ?achievements)",
    "(count ?courses)",
    "(count ?degrees)",
    "(count ?licenses)"
  ],
  "where": [{
    "union": [
      [["?learners", "acd:profileType", "Learner"]],
      [{
        "union": [
          [["?institutions", "acd:profileType", "Institution"]],
          [{
            "union": [
              [["?employers", "acd:profileType", "Employer"]],
              [{
                "union": [
                  [["?achievements", "type", "clr:AchievementSubject"]],
                  [{
                    "union": [
                      [["?courses", "clr:achievementType", "Course"]],
                      [{
                        "union": [
                          [["?degrees", "clr:achievementType", "Degree"]],
                          [["?licenses", "clr:achievementType", "License"]]
                        ]
                      }]
                    ]
                  }]
                ]
              }]
            ]
          }]
        ]
      }]
    ]
  }]
}
